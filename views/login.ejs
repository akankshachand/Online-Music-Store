<% include partials/header %>
<div class="wrap">
<br><h1 class="title">User Login</h1>
<br><br>
<form action="/login"  method=POST>
  <div class="form-group">
    <input type="email" name="username" class="form-control" placeholder="Enter email" style="width:300" required>
  </div>
  <div class="form-group">
    <input type="password" name="password" class="form-control" placeholder="Password" style="width:300" required>
  </div>
  
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
</div>

<% include partials/footer %>